<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spring.redis.mapper.UserMapper">
    <select id="getLists" resultType="com.spring.redis.entity.redisuser">
        SELECT * FROM redisuser
    </select>
    <insert id="add" parameterType="com.spring.redis.entity.redisuser">
        INSERT  INTO redisuser (name,sex,age)VALUES (#{name},#{sex},#{age})
    </insert>
</mapper>